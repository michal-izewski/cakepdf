{
    "name": "michal-izewski/cakepdf",
    "description": "CakePHP plugin for converting HTML to PDF",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name" : "Michał Iżewski",
            "homepage": "http://www.iamdev.pl"
        },
        {
            "name": "NVB",
            "homepage": "http://nvb-online.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "knplabs/knp-snappy": "^0.5.0",
        "h4cc/wkhtmltopdf-i386": "^0.12.4",
        "h4cc/wkhtmltoimage-i386": "^0.12.4",
        "zendre4/wkhtmltopdf-amd64": "^0.12.6",
        "silvertipsoftware/wkhtmltoimage-amd64": "^0.12.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Guenbakku\\Cakepdf\\": "src"
        }
    },
    "config": {
		"vendor-dir": "vendors/"
	}
}
